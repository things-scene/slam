{
  "name": "@things-elements/things-scene-slam",
  "flat": true,
  "version": "0.1.0",
  "description": "An element providing a starting point for your own reusable Polymer elements.",
  "main": "things-scene-slam-min.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "serve": "watchify src/*.js -o things-scene-slam.js src/index.js & polyserve",
    "test": "./node_modules/.bin/mocha --require should --compilers js:babel-core/register test/**/test-*.js",
    "build": "browserify src/index.js -o things-scene-slam.js && uglifyjs things-scene-slam.js -o things-scene-slam-min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThingsElements/things-scene-slam.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ThingsElements/things-scene-slam/issues"
  },
  "homepage": "https://github.com/ThingsElements/things-scene-slam#readme",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "~7.2.0",
    "browserify": "~10.2.4",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "should": "^8.0.2",
    "uglify-js": "^2.4.11",
    "watchify": "^3.3.1",
    "node-canvas": "Automattic/node-canvas"
  },
  "dependencies": {
    "@hatiolab/things-scene": "^2.2.36"
  }
}
